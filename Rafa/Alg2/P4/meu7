funcao insereOrdenada(s:string;v:inteiro):logico
var
	fOriginal,fNovo:sequencia de inteiro
	valorAtual:inteiro
	inserido: logico
inicio
	inserido <- FALSO
	se abre(fOriginal,s) entao
		se cria(fNovo,"temp.dat") entao
			enquanto nao fds(fOriginal) faca
				leia(fOriginal,valorAtual)
				se v < valorAtual e nao inserido entao
					escreva(fNovo,v)
				fimse
				escreva(fNovo,valorAtual)
			fim enquanto
			se nao inserido entao
				escreva(fNovo,v)
			fimse
			fecha(fNovo)
		senao
			retorne FALSO
		fimse
		fecha(fOriginal)
		apaga(s)
		renomeia("temp.dat",s)
	senao
		retorne FALSO
	fimse
	retorne VERDADEIRO
fim funcao
