funcao moda(s:string):inteiro
var
	v:vetor[0..3000] de inteiro
	nModa,vAtual,i:inteiro
	f:sequencia de inteiro
inicio
	para i de 0 ate 3000 faca
		v[i] <- 0
	fimpara
	se abre(f,s) entao
		enquanto nao fds(f) faca
			leia(f,vAtual)
			v[vAtual] <- v[vAtual] + 1
		fim enquanto
		fecha(f)
	senao
		retorne -9999
	fimse
	nModa <- 0
	para i de 0 ate 3000 faca
		se v[i] > nModa entao
			nModa <- v[i]
		fimse
	fim para
	i <- 3000
	enquanto i >= 0 fa√ßa
		se v[i] = nModa entao
			retorne i
		fimse
		i <- i -1
	fim enquanto
fim funcao
