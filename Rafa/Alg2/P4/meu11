funcao somatorioSequencias(s:string):inteiro
var
	valorAtual,soma:inteiro
	sAtual:string
	fs:sequencia de string
	fi:sequencia de inteiro
inicio
	soma <- 0 
	se abre(fs,s) entao
		enquanto nao fds(fs) faca
			leia(fs,sAtual)
			se abre(fi,sAtual) entao
				enquanto nao fds(fi) faca
					leia(fi,valorAtual)
					soma <- soma + valorAtual
				fim enquanto
				fecha(fi)
			senao
				pare
			fimse
		fim enquanto
		fecha(fs)
		retorne soma
	senao
		pare
	fimse
fim funcao
					
		
