funcao trocaPosição(s:string;p:inteiro):logico
var
	fOriginal,fNovo:sequencia de real
	v,vp:real
	pAtual:inteiro
inicio
	pAtual <- 1
	se abre(fOriginal,s) entao
		enquanto nao fds(fOriginal) faca
			leia(fOriginal,v)
			pAtual <- pAtual + 1
		fim enquanto
	senao
		retorne FALSO
	fimse
	fecha(fOriginal)
	se pAtual >= p + 1 e cria(fNovo,"temp.dat") entao
		pAtual <- 1
		se abre(fOriginal,s) entao
			enquanto nao fds(fOriginal) faca
				leia(fOriginal,v)
				se pAtual = p entao
					vp <- v
					leia(fOrignal,v)
					escreva(fNovo,v,vp)
				senao
					escreva(fNovo,v)
				fimse
				pAtual <- pAtual + 1
			fim enquanto
		fimse	
		fecha(fOriginal)
		fecha(fNovo)
		apaga(s)
		renomeia("temp.dat",s)
		retorne VERDADEIRO
	senao
		retorne FALSO
	fimse
fim funcao
