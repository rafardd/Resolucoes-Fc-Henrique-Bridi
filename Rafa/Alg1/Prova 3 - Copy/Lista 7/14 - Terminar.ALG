Algoritmo "FormataUmNomePróprio"

Var
   s, final, c : caractere
   i : inteiro

Inicio
      escreva("Insira um nome: ")
      leia(s)
      i <- 1
      final <- ""
      enquanto i <= compr(s) faça
               // todo
               i <- i + 1
      fimenquanto
      escreva("Nome revisado: ", final)
Fimalgoritmo