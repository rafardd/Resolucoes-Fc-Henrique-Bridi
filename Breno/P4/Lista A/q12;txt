algoritmo SorteioMoedaViciada
var
    lado_moeda: real
    sortear: logico
inicio
    sortear <- VERDADEIRO
    enquanto sortear faÃ§a
        sortear <- FALSO
        lado_moeda <- aleatorio()
        se lado_moeda > 0.65 entao
            escreva("Coroa")
        senao
            se lado_moeda < 0.65 entao
                escreva("Cara")
            senao
                sortear <- VERDADEIRO
            fim se
        fim se
    fim enquanto
fim algoritmo